{
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.2"
      },
      "api/*.php": {
        "runtime": "vercel-php@0.7.2"
      }
    },
    "routes": [
      {
        "src": "/views/(.*)",
        "dest": "/views/$1"
      },
      {
        "src": "/controllers/(.*)",
        "dest": "/controllers/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }
  